{
	"compilerOptions": {
		"strict": true,
		"target": "ES2022",
		"module": "ES2022",
		"lib": [
			"ES2022"
		],
		"allowJs": true,
		"esModuleInterop": true,
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"useUnknownInCatchVariables": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"baseUrl": "./src",
		"outDir": "./dist",
		"skipLibCheck": true
	}
}